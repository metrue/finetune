{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
     "sourceMap": true,
     "outDir": "./dist",
     "rootDir": "./src",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules","__tests__"]
}
